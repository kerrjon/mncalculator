@model MnCalculator.Models.CalculatorResult
@{
  ViewBag.Title = "Minnesota Child Support Calculator Results - Bill HF2889";
  ViewBag.Description = "The resulting outcome of Minnesota Child Support Calculator based on Minnesota House Bill HF2889.";
}

<h2>Calculator Results</h2>
<div class="form-horizontal">
  <div class="col-md-5 col-sm-5">
    <dl class="dl-horizontal">
      <dt>Parent A Name</dt>
      <dd>@Html.DisplayFor(model => model.ParentA.Name)</dd>

      <dt>Parent B Name</dt>
      <dd>@Html.DisplayFor(model => model.ParentB.Name)</dd>

      <dt>@Html.DisplayNameFor(model => model.NumberOfChildren)</dt>
      <dd>@Html.DisplayFor(model => model.NumberOfChildren)</dd>
    </dl>
  </div>

  <div class="col-md-5 col-sm-5 col-md-offset-2 col-sm-offset-2">
    <dl class="dl-horizontal">
      <dt>@Html.DisplayNameFor(model => model.CaseNumber)</dt>
      <dd>@Html.DisplayFor(model => model.CaseNumber)</dd>

      <dt>@Html.DisplayNameFor(model => model.CourtFileNumber)</dt>
      <dd>@Html.DisplayFor(model => model.CourtFileNumber)</dd>
    </dl>
  </div>
</div>

<div class="row resultRow headerRow" style="clear:both">
  <span class="col-md-6 col-sm-6 col-xs-6">&nbsp;</span>
  <span class="col-md-2 col-sm-2 col-xs-2">Parent&nbsp;A</span>
  <span class="col-md-2 col-sm-2 col-xs-2">Parent&nbsp;B</span>
  <span class="col-md-2 col-sm-2 col-xs-2">Combined</span>
</div>

<div class="col-md-12 col-sm-12 col-xs-12 category">
  <h2>Income Summary</h2>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.MonthlyIncomeReceived)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.MonthlyIncomeReceived)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.MonthlyIncomeReceived)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.SocialSecurityBenefits)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.SocialSecurityBenefits)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.SocialSecurityBenefits)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.PotenialIncome)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.PotenialIncome)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.PotenialIncome)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.SpousalMaintenance)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.SpousalMaintenance)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.SpousalMaintenance)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.NonJointChildSupport)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.NonJointChildSupport)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.NonJointChildSupport)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.Combined.NonJointChildSupport)</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.GrossIncome)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.GrossIncome)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.GrossIncome)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.Combined.GrossIncome)</span>
  </div>
</div>

<div class="col-md-12 col-sm-12 col-xs-12 category">
  <h2>Parenting Time Summary</h2>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.Overnights)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentA.Overnights)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentB.Overnights)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.Combined.Overnights)</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.PercentageOfParentingTime)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentA.PercentageOfParentingTime)%</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentB.PercentageOfParentingTime)%</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.Combined.PercentageOfParentingTime)%</span>
  </div>
</div>

<div class="col-md-12 col-sm-12 col-xs-12 category">
  <h2>Adjustments</h2>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.NumberOfNonJointChildren)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentA.NumberOfNonJointChildren)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentB.NumberOfNonJointChildren)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.DeductionForNonJointChildren)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentA.DeductionForNonJointChildren)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentB.DeductionForNonJointChildren)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.PicsAmount)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.PicsAmount)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.PicsAmount)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.Combined.PicsAmount)</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.PicsPercentage)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentA.PicsPercentage)%</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.ParentB.PicsPercentage)%</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.Combined.PicsPercentage)%</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.CombinedBasicSupportObligation)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
    <span class="col-md-2 col-sm-2 col-xs-2">@Html.DisplayFor(model => model.CombinedBasicSupportObligation)</span>
  </div>
  <div class="row resultRow">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.ProRataBasicSupportObligation)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.ProRataBasicSupportObligation)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.ProRataBasicSupportObligation)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
  <div class="row resultRow highlighted">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.BasicSupportObligationAfterAdjustment)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.BasicSupportObligationAfterAdjustment)</span>
    @if (Model.ParentB.BasicSupportObligationAfterAdjustment > 0 && Model.ParentB.PercentageOfParentingTime > 55)
    {
      <span class="col-md-2 col-sm-2 col-xs-2">$0 *</span>
    }
    else
    {
      <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.BasicSupportObligationAfterAdjustment)</span>
    }
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
</div>

@if (Model.ParentB.BasicSupportObligationAfterAdjustment > 0 && Model.ParentB.PercentageOfParentingTime > 55)
{
  <p class="col-md-12 text-center" style="font-size: smaller">* Obligation for Parent B is reduced to 0 because parenting time is above 55%.</p>
}

<hr/>
<h2>Legacy Calculation</h2>
<p>The result below respresents the old method of child support calculation used <a href="http://childsupportcalculator.dhs.state.mn.us/" title="Minnesota Child Support Calculator">here</a>.</p>
<div class="col-md-12 col-sm-12 col-xs-12 category">
  <div class="row resultRow highlighted">
    <span class="col-md-6 col-sm-6 col-xs-6">@Html.DisplayNameFor(model => model.ParentA.LegacyObligation)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentA.LegacyObligation)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">$@Html.DisplayFor(model => model.ParentB.LegacyObligation)</span>
    <span class="col-md-2 col-sm-2 col-xs-2">----</span>
  </div>
</div>

<p style="margin-top: 100px">If you see a problem with the calculator, please report issues to <a href="mailto:info@mncalculator.com">info@mncalculator.com</a></p>

